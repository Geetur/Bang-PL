fn max_sum args
    ans = 0
    for i args[0]
        ans = max{ans, sum{i}}
    end
    return ans
end

fn rob args
    nums = args[0]

    if len{nums} <= 2
        return max{nums}
    end

    nums[-2] = max{nums[-1], nums[-2]}

    for range1 [len{nums} - 3, -1, -1]
        nums[range1] = max{nums[range1] + nums[range1 + 2], nums[range1 + 1]}
    end

    return nums[0]
end

